# WCleaner bash-completion

_wclean_completion() {
    local cur opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    opts="--help --version"

    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
    return 0
}

echo "source ~/WClean/.bash_completion" >> ~/.bashrc

complete -F _wclean_completion wclean
